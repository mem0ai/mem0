{
        "vector_store": {
            "provider": "pgvector",
            "config": {
                "user": "env:DB_USER",
                "password": "env:DB_PASSWORD",
                "host": "env:DB_HOST",
                "port": "5432"
            }
        },
        "llm": {
            "provider": "openai",
            "config": {
                "model": "gpt-4o-mini",
                "temperature": 0.1,
                "max_tokens": 2000,
                "api_key": "env:OPENAI_API_KEY"
            }
        },
        "embedder": {
            "provider": "openai",
            "config": {
                "model": "text-embedding-3-small",
                "api_key": "env:OPENAI_API_KEY"
            }
        },
        "version": "v1.1"
    }
